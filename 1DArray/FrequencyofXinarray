public class Solution {
    public int solve(int[] A, int B) {
        int count = 0;
        for ( int i = 0; i < A.length; i++) {
            if (A[i] == B) {
                count++;
            }
        }
        return count;
    }
}

class Solution:
    # @param A : list of integers
    # @param B : integer
    # @return an integer
    def solve(self, A, B):
        ans = 0
        for i in A:
            # incrementing ans by 1
            if i == B: 
                ans += 1
        return ans
